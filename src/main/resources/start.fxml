<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane hgap="100" vgap="50"
          prefHeight="500.0" prefWidth="600.0"
          styleClass="StartGridPane"
          xmlns="http://javafx.com/javafx/11.0.1"
          xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="at.turtles.StartController">

    <gridLinesVisible>true</gridLinesVisible>
    <stylesheets>
        <URL value="@style.css" />
    </stylesheets>

    <!-- TODO: Design -->
    <Label fx:id="titleLabel" text="Save the Animals" GridPane.columnIndex="1" GridPane.rowIndex="3" />
    <Button fx:id="startButton" onAction="#startButtonclicked" styleClass="button1" text="Start" GridPane.columnIndex="0" GridPane.rowIndex="5" />
    <Button fx:id="exitButton" onAction="#onExit" styleClass="button2" text="Exit" GridPane.columnIndex="2" GridPane.rowIndex="5" />
    <columnConstraints>
        <ColumnConstraints maxWidth="127.19999999999999" minWidth="26.200003814697254" prefWidth="68.40001525878907" />
        <ColumnConstraints maxWidth="72.66667429606119" minWidth="0.0" prefWidth="17.999984741210938" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
        <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0" />
        <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0" />
        <RowConstraints />
    </rowConstraints>


</GridPane>
